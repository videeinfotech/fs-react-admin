{
  "name": "find-sukoon-admin-panel",
  "private": true,
  "version": "1.0.0",
  "description": "A responsive admin panel for the Find Sukoon emotional support platform, built with React and Tailwind CSS. It includes features for managing users, listeners, sessions, transactions, and more.",
  "engines": {
    "node": "18.x",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "shopify hydrogen dev",
    "build": "shopify hydrogen build",
    "preview": "shopify hydrogen preview",
    "check-deps": "npm ls",
    "clean": "rm -rf node_modules package-lock.json && npm cache clean --force",
    "install:clean": "npm run clean && npm install"
  },
  "dependencies": {
    "@shopify/hydrogen": "2024.4.2",
    "quill": "1.3.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-quill": "3.0.0-beta.2",
    "react-router-dom": "6.23.0",
    "recharts": "2.12.5"
  },
  "devDependencies": {
    "@netlify/plugin-hydrogen": "^4.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "typescript": "^5.4.5",
    "vite": "^5.2.11"
  }
}